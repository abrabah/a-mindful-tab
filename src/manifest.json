{
  "manifest_version": 2,
  "name": "a mindful tab",
  "version": "1.1",
  "description": "Replace your new tab with nice nature images and quotes ",
  "chrome_url_overrides": {
    "newtab": "mindful.html"
  },
  "permissions": [
    "http://feeds.feedburner.com/brainyquote/QUOTEBR",
    "https://i.redd.it/*"
  ],
  "background": {
    "scripts": [
      "backend.js"
    ]
  },
  "web_accessible_resources": [
    "style.css",
    "frontend.js",
    "mindful.html"
   ]
}
